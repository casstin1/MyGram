<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyGram</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">MyGram</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <button>Log in</button><button>Sign Up</button>
            </div>
        </nav>
    </header>
    <table>
        <thead>
            <tr>
                <th>
                    Images
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    <img style="width:300px; height:300px;"
                         src="https://s3.amazonaws.com/cdn-origin-etr.akc.org/wp-content/uploads/2017/11/12224830/Doberman-Pinscher-standing-outdoors.jpg"/>
                </td>
                <td>
                    <img style="width:300px; height:300px;"
                         src="https://i.pinimg.com/originals/54/03/c0/5403c00827a8228b618a37eccbd3c987.jpg"/>
                </td>
                <td>
                    <img style="width:300px;height:300px;"
                         src="https://hellobark.com/wp-content/uploads/the-doberman-team-1.jpg"/>
                </td>
                </tr>
            <tr>
                <td>
                    <img style="width:300px; height:300px;"
                         src="https://cdn.fstoppers.com/styles/full/s3/photos/195258/07/15/455d72f260d95f37f02fd500ccaa069c.jpg"/>
                </td>
                <td>
                    <img style="width:300px; height:300px;"
                         src="https://i.pinimg.com/originals/ce/02/2b/ce022bec57893b1681acb2b3a32ef438.jpg"/>
                </td>
                <td>
                    <iframe width="300px" height="300px;"
                         src="https://www.youtube.com/embed/_5SUJzf3sxs" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>/>
                </td>

            </tr>
        </tbody>
    </table>
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
        <footer class="border-top footer text-muted">
            <a href="" target="_blank">About</a>
            <a href="" target="_blank">Blogs</a>
            <a href="" target="_blank">Jobs</a>
            <a href="" target="_blank">Help</a>
            <a href="" target="_blank">English</a>
            <a href="" target="_blank">2021</a>
            <a href="" target="_blank">MyGram</a>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @RenderSection("Scripts", required: false)
</body>
</html>
